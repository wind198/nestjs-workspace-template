{
  "name": "nestjs-workspace-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build:central-web-server": "nest build central-web-server",
    "start:central-web-server:dev": "nest start central-web-server --watch",
    "start:central-web-server:debug": "nest start central-web-server --debug --watch",
    "start:central-web-server:prod": "node dist/apps/central-web-server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run prisma:reset:test && npm run prisma:migrate:test && jest",
    "email:dev": "email dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:reset:test": "dotenv -e .env.test prisma migrate reset -- --force",
    "prisma:migrate:test": "dotenv -e .env.test prisma migrate dev",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "ts-node": "ts-node"
  },
  "dependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.9.1",
    "@faker-js/faker": "^10.1.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.17.1",
    "@react-email/components": "0.5.6",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/moment": "^2.13.0",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "nest-winston": "^1.10.2",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-amazon": "^1.0.0",
    "prisma": "^6.17.1",
    "qs": "^6.14.0",
    "query-types": "^0.1.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@react-email/preview-server": "4.3.0",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "react-email": "4.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": false
        }
      ],
      "^.+\\.tsx$": [
        "ts-jest",
        {
          "useESM": false
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@faker-js|uuid)/)"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@app/config": "<rootDir>/libs/config/src",
      "^@app/config/(.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/emails": "<rootDir>/libs/emails/src",
      "^@app/emails/(.*)$": "<rootDir>/libs/emails/src/$1",
      "^@app/prisma": "<rootDir>/libs/prisma/src",
      "^@app/prisma/(.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/server/(.*)$": "<rootDir>/apps/server/src/$1",
      "^@app/server/*": "<rootDir>/apps/server/src/*",
      "^generated/prisma$": "<rootDir>/generated/prisma",
      "^generated/prisma/$": "<rootDir>/generated/prisma/",
      "^uuid$": "<rootDir>/__mocks__/uuid.js",
      "^@react-email/components$": "<rootDir>/__mocks__/@react-email/components.js"
    }
  }
}
